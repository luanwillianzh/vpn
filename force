  read -p "QUER BUSCAR MAIS ALGUMA INFO? [Y/n]" optione12 ; case $optione12 in 
  y)PegarinfoAll2;;
  Y)PegarinfoAll2;;
  *)Voltar;;
  esac
}
Voltar(){
echo
read -n1 -p "APERTE QUALQUER TECLA PARA VOLTAR" optione1
  case $optione1 in
  *)X9sniffer;;
  esac
}


TodasPortasAll(){
clear
