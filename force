clear

echo -e "${GREEN}CAPTURA INICIADA...${SCOLOR}" ; echo ; echo -e "${DESTACAR}PARA PARAR DIGITE${NADA} ${GREEN}CTRL+C${NADA}"

Sniff=$(su -c tcpdump -c10000 -A)
echo -e "$Sniff" >> sniffer.txt
cat sniffer.txt


Pegarinfo2All
}
Pegarinfo2All(){
